{
  "name": "wagopa",
  "version": "1.0.0",
  "license": "MIT",
  "main": "index.ts",
  "private": true,
  "scripts": {
    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "build": "npx tsc",
    "build:prod": "npm run clean && npm run build && npm run docs",
    "build:dev": "npm run build",
    "docs": "npm run docs:md",
    "docs:md": "npx typedoc --plugin typedoc-plugin-markdown --theme markdown --out docs",
    "clean": "npx rimraf ./docs ./build ./.build.json",
    "cover": "npm run cover:unit && npm run cover:integration && npm run cover:report",
    "cover:unit": "nyc --silent npm run test:unit",
    "cover:integration": "nyc --silent --no-clean npm run test:integration",
    "cover:report": "nyc report --reporter=lcov --reporter=text"
  },
  "dependencies": {
    "@babel/runtime": "^7.27.1",
    "@expo/vector-icons": "^14.1.0",
    "@react-native-community/datetimepicker": "^8.3.0",
    "@react-native-picker/picker": "^2.11.0",
    "@react-navigation/bottom-tabs": "^7.3.13",
    "@react-navigation/native": "^7.1.9",
    "@react-navigation/native-stack": "^7.3.13",
    "@react-navigation/stack": "^7.3.2",
    "@wago/wdx-schema": "^24.0.0",
    "@wago/wdx-ws-client-js": "^24.0.0",
    "expo": "53.0.9",
    "expo-barcode-scanner": "~12.7.0",
    "expo-camera": "~16.1.6",
    "expo-dev-client": "~5.1.8",
    "expo-document-picker": "~13.1.5",
    "expo-image-picker": "~16.1.4",
    "expo-location": "~18.1.4",
    "expo-status-bar": "~2.2.3",
    "install": "^0.13.0",
    "nativewind": "^4.1.23",
    "npm": "^11.3.0",
    "npx": "^10.2.2",
    "react": "19.0.0",
    "react-native": "0.79.2",
    "react-native-calendars": "^1.1312.0",
    "react-native-chart-kit": "^6.12.0",
    "react-native-gesture-handler": "~2.24.0",
    "react-native-maps": "1.20.1",
    "react-native-reanimated": "~3.17.4",
    "react-native-safe-area-context": "5.4.0",
    "react-native-screens": "~4.10.0",
    "react-native-svg": "^15.12.0",
    "react-native-vector-icons": "^10.2.0",
    "react-native-websocket": "^1.0.2",
    "rxjs": "^7.8.2",
    "twrnc": "^4.8.0",
    "uuid": "^9.0.1",
    "websocket": "^1.0.35"
  },
  "devDependencies": {
    "@babel/core": "^7.27.1",
    "@react-native-community/cli": "^18.0.0",
    "@types/node": "^20.5.9",
    "@types/react": "~19.0.10",
    "@types/react-native-vector-icons": "^6.4.18",
    "@types/uuid": "^9.0.4",
    "@types/websocket": "^1.0.10",
    "babel-plugin-module-resolver": "^5.0.2",
    "concat-md": "^0.5.0",
    "metro-react-native-babel-preset": "^0.77.0",
    "nodemon": "^3.0.2",
    "nyc": "^15.1.0",
    "rimraf": "^5.0.1",
    "showdown": "^2.1.0",
    "ts-node": "^10.9.2",
    "typedoc": "^0.25.6",
    "typedoc-plugin-markdown": "^3.14.0",
    "typescript": "^5.4.5"
  }
